{
  "kind": "collectionType",
  "collectionName": "salam_job_listings",
  "info": {
    "singularName": "salam-job-listing",
    "pluralName": "salam-job-listings",
    "displayName": "Job",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "jobTitle": {
      "type": "string",
      "required": true
    },
    "jobSummary": {
      "type": "text"
    },
    "jobCode": {
      "type": "string"
    },
    "department": {
      "type": "enumeration",
      "enum": [
        "Technical",
        "Manager",
        "Testing",
        "Sales",
        "Management",
        "Marketting"
      ]
    },
    "jobDescription": {
      "type": "richtext"
    },
    "candidates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::form-submission.form-submission",
      "mappedBy": "job"
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "Central",
        "Northern",
        "Northwest",
        "Midwest",
        "Southwest"
      ]
    },
    "applicationCount": {
      "type": "integer"
    },
    "jobLevel": {
      "type": "enumeration",
      "enum": [
        "Trainee",
        "Junior",
        "Manager",
        "Senior"
      ]
    }
  }
}
