{
  "kind": "collectionType",
  "collectionName": "salam_job_listings",
  "info": {
    "singularName": "salam-job-listing",
    "pluralName": "salam-job-listings",
    "displayName": "Job",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "jobCode": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Technical",
        "Manager",
        "Testing",
        "Sales",
        "Management",
        "Marketting"
      ]
    },
    "longDescription": {
      "type": "richtext"
    },
    "candidates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::form-submission.form-submission",
      "mappedBy": "job"
    }
  }
}
