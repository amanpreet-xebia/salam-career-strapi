{
  "kind": "collectionType",
  "collectionName": "form_submissions",
  "info": {
    "singularName": "form-submission",
    "pluralName": "form-submissions",
    "displayName": "Candidate",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "maxLength": 18,
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "nationality": {
      "type": "string",
      "required": true
    },
    "totalYearsOfExperience": {
      "type": "string"
    },
    "totalYearsOfRelevantExperience": {
      "type": "string"
    },
    "currentCompany": {
      "type": "string"
    },
    "currentJobTitle": {
      "type": "string"
    },
    "CV": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "linkedInUrl": {
      "type": "string"
    },
    "isWorkExperience": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "Gender": {
      "type": "string",
      "required": true
    },
    "jobTitle": {
      "type": "string"
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::salam-job-listing.salam-job-listing",
      "inversedBy": "candidates"
    },
    "jobCode": {
      "type": "string"
    },
    "professionalCertificate": {
      "type": "json"
    },
    "educationInformation": {
      "displayName": "Education Information",
      "type": "component",
      "repeatable": true,
      "component": "education-information.education-information"
    },
    "isIqamaTransferable": {
      "type": "string"
    },
    "jobInIqama": {
      "type": "string"
    }
  }
}
